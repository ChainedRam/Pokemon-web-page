{
  "name": "pokemon-type-calc",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.3",
    "jQuery": "^1.7.4",
    "react": "^16.5.1",
    "react-dom": "^16.5.1"
  },
  "engines": {
    "npm": "6.1.0",
    "node": "10.3.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "compileMining": "tsc -p .\\mining\\tsconfig.json",
    "gather:poke": "npm run compileMining && node .\\mining\\compiled\\gatherPokemons.js",
    "gather:type": "npm run compileMining && node .\\mining\\compiled\\gatherTypes.js",
    "gather:move": "npm run compileMining && node .\\mining\\compiled\\gatherMoves.js",
    "serve:b": "npm run build && serve -s build",
    "serve": "serve -s build"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "@types/node-fetch": "^2.1.2",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-test-renderer": "^16.0.2",
    "@types/reactstrap": "^6.0.3",
    "node-fetch": "^2.2.0",
    "react-scripts-ts": "2.17.0",
    "react-test-renderer": "^16.5.2",
    "reactstrap": "^6.4.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  }
}
